/**index.wxss**/
.container{padding:15rpx;}
.bannerMain{position:relative;width:720rpx;height:697rpx;background:#aaa;border-radius:15rpx;}
.bannerImg{width:720rpx;height:697rpx;border-radius:15rpx;}
.left-layout{position:absolute;left:0;bottom:0;height:160rpx;width:100%;background:rgba(0,0,0,0.2);border-radius:0 0 15rpx 15rpx;}
.img-icon1{position:absolute;left:35rpx;top:35rpx;width:98rpx;height:98rpx;border-radius:15rpx;}
.bannerTxt{position:absolute;top:40rpx;left:165rpx;color:#fff;font-size:32rpx;line-height:1.5em;display:block;width:400rpx;}
.bannerTxt text{display:block;}
.bannerTxt .title{font-size:42rpx;}

.swiperClass{
  width: 720rpx;
  height: 310rpx;
}
swiper-item{
  padding-top:10rpx;
}
.swiperClass .slide-image{
  width: 690rpx;
  height: 295rpx;
  position: absolute;
  left:15rpx;
}
.swiperMain{height:295rpx;margin-right:35rpx;
  background:#fff;
  border-radius:10rpx;
  box-shadow:0 0 15rpx rgba(0,0,0,0.2);
}

.swiperClass .slide-image.active{
  /* transform: scale(1.14); */
  transition:all .2s ease-in 0s;
  border-radius:10rpx;
}
.swiperFlex{padding:30rpx 30rpx 20rpx;font-size:30rpx;}
.swiperFlex .icon{width:98rpx;height:98rpx;margin-right:20rpx;}
.swiperFlex .title{margin:5rpx 0;font-size:34rpx;display:block;}
.starArr{width:5em;height:1em;line-height:0;background-image:url(data:image/png;base64,);background-repeat:repeat-x;background-size:1em auto;}
.swiperFlex .star{display:block;height:1em;background-image:url(data:image/png;base64,);background-size:1em auto;}
.swiperTxt{height:120rpx;padding:0 30rpx;font-size:28rpx;overflow-y:auto;}
.indexList{margin-top:40rpx;margin:40rpx 25rpx;background:#fff;border-radius:10rpx;box-shadow:0 0 20rpx rgba(0,0,0,0.2)}
.listImg{}
.listImg image{width:100%;height:340rpx;border-radius:10rpx 10rpx 0 0;}
.indexFlex{padding:0 20rpx 15rpx;}
.indexFlex .title{flex:1;font-size:28rpx;color:#868686;}
.indexFlex .tl{display:block;font-size:32rpx;color:#333;}
.indexFlex .pf{margin-top:17rpx;color:#ff8d54;font-size:34rpx;vertical-align:middle;}
.indexFlex .pfImg{width:34rpx;height:34rpx;margin-right:10rpx;vertical-align:-5rpx;}



.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}